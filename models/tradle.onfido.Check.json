{
  "type": "tradle.Model",
  "id": "tradle.onfido.Check",
  "title": "Onfido Check",
  "subClassOf": "tradle.Check",
  "properties": {
    "applicant": {
      "type": "object",
      "ref": "tradle.Identity"
    },
    "application": {
      "type": "object",
      "ref": "tradle.Application"
    },
    "selfie": {
      "type": "object",
      "ref": "tradle.Selfie"
    },
    "photoID": {
      "type": "object",
      "ref": "tradle.PhotoID"
    },
    "applicantDetails": {
      "type": "array",
      "items": {
        "ref": "tradle.Form"
      }
    },
    "inputs_group": {
      "type": "string",
      "title": "Data sent to Onfido",
      "list": [
        "selfie",
        "photoID",
        "applicantDetails"
      ]
    },
    "faceMatchConfidence": {
      "type": "number"
    },
    "rawData": {
      "type": "object",
      "range": "json"
    },
    "errors": {
      "type": "object",
      "range": "json"
    },
    "reportsOrdered": {
      "type": "array",
      "inlined": true,
      "items": {
        "ref": "tradle.onfido.ReportType"
      }
    },
    "status": {
      "type": "object",
      "ref": "tradle.Status",
      "readOnly": true
    },
    "dateChecked": {
      "type": "date"
    },
    "onfidoStatus": {
      "type": "object",
      "ref": "tradle.onfido.CheckStatus"
    },
    "onfidoResult": {
      "type": "object",
      "ref": "tradle.onfido.OpResult"
    },
    "onfidoApplicant": {
      "type": "object",
      "range": "json"
    },
    "onfidoCheckId": {
      "type": "string"
    },
    "message": {
      "type": "string",
      "readOnly": true,
      "displayName": true
    }
  },
  "indexes": [
    {
      "hashKey": "_t",
      "rangeKey": "_time"
    },
    {
      "hashKey": "_permalink",
      "rangeKey": {
        "template": "_"
      }
    },
    {
      "hashKey": "application._permalink",
      "rangeKey": "_permalink"
    },
    "onfidoCheckId"
  ],
  "required": [
    "applicant",
    "application",
    "reportsOrdered"
  ],
  "viewCols": [
    "status",
    "message",
    "faceMatchConfidence",
    "dateChecked",
    "reportsOrdered",
    "inputs_group",
    "rawData"
  ]
}
